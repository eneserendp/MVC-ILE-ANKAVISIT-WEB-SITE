@using TravelTripProje.Models.Siniflar
@model List<Hakkimizda>
@{
    ViewBag.Title = "HakkimizdaListesi";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Hakkımızda Listesi</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>FOTOURL</th>
        <th>AÇIKLAMA</th>
        <th>Güncelle</th>
        

    </tr>
    @foreach (var x in Model)
    {





        <tr>
            <td>@x.Id</td>
            <td>@x.FotoUrl</td>
            <td>@x.Aciklama</td>
            
            
            <td><a href="/Admin/HakkimizdaGetir/@x.Id" class=" btn btn-warning"></a>Güncelle</td>
            

            

        </tr>
    }


</table>



